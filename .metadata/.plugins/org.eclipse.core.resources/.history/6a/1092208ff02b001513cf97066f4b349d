package com.online.Interitance;

public class finalprogram {

	
	public static void main(String[] args) {
		Car c1=new Car();
		c1.brand = "Lexus";
		c1.make = 2015;
		c1.model="Standard";
		c1.totalDoor = 2;
		Car b1 = new Bike();
		b1.brand = "Yamaha";
		b1.make = 2000;
		b1.model = "R1";
		b1.cc = 88;
		b1.bhp = 1200;
		System.out.println("Car details" +c1.brand );
	}

}
