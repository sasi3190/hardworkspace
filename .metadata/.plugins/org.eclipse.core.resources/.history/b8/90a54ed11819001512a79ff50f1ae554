package com.online.practise;

public class Student {

	public static void main(String args[]) {
		int lastNameidx;
		String lastName;
		String SName="Sasi kumar";
		System.out.println("Complete name:"+SName);
	lastNameidx=SName.indexOf(null)+1;
		lastName=SName.substring(lastNameidx);
	System.out.println("last name:"+lastName);
		
		

	}

}
